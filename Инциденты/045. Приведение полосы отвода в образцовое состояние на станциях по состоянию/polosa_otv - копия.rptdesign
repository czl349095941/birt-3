            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();

var siteid = new String(params["userName"]);

siteid = " ( select distinct defsite from maximo.maxuser where loginid = '" + params["userName"] + "' )";
//siteid = "24"


var start = new String(params["par"]);
start = "to_date ('"+params["par"]+"','DD.MM.YY')"


	sqlText =  " with "
	
 +"   loc as "
 +"  ( select lo.location, spc2.alnvalue||', '||spc3.alnvalue description, spc1.NUMVALUE nom, 'P-'||reg.region as region "

 +"     from  maximo.LOCATIONSPEC spc  "
 +"   join MAXIMO.locations lo       on lo.location = spc.location and spc.siteid = lo.siteid     "
 +"   and spc.ASSETATTRID = 'TIP_PREDPR' and spc.NUMVALUE in (3)    "
 +"   join maximo.LOCATIONSPEC spc1 on lo.location = spc1.location and spc1.siteid = lo.siteid     "
 +"   and spc1.ASSETATTRID = 'NOM'     and spc.siteid =  " + siteid 
 +"    left join maximo.LOCATIONSPEC spc2 on lo.location = spc2.location and spc2.siteid = lo.siteid "
 +"   and spc2.ASSETATTRID = 'SNAME' "
 +"   left join maximo.LOCATIONSPEC spc3 on lo.location = spc3.location and spc3.siteid = lo.siteid  "
 +"   and spc3.ASSETATTRID = 'SNAME2' "
 
   +"   left join  ( select lo.parent, lo.siteid, lo.location, spc.numvalue region  from MAXIMO.lochierarchy lo "
  +"   					join maximo.LOCATIONSPEC spc3 on lo.parent = spc3.location and spc3.siteid = lo.siteid and spc3.ASSETATTRID = 'TIP_PREDPR' "
  +"   					join maximo.LOCATIONSPEC spc on lo.parent = spc.location and spc.siteid = lo.siteid and spc.ASSETATTRID = 'NOM' "  
  +"				 where  lo.systemid='ОС' and spc3.numvalue=2)reg on lo.location = reg.location and reg.siteid = lo.siteid "
 
 +"     where  lo.siteid =   " + siteid + "     and lo.status = 'РАБОТАЕТ'      "
 +"   			and lo.location like 'PRED%'   "
 +"    ), "

              
            
+" inc_year as (select count(distinct i.ticketid) inc_count, i.CLASSIDMAIN, (case when i.extsystemid is not null then nvl( ma.pred, i.pred) else ma.pred end ) pred,  i.status, "
+"			( CASE WHEN  to_date (   i.reportdate,'DD.MM.YY') = " + start + " THEN 1 "
+"				else 0 end ) today "	
+" from maximo.incident  i "

+"        	 join (select RECORDKEY, RECORDCLASS, PODR, ISPRIMARY, SITEID, pred from  MAXIMO.multiassetlocci "
+"						) ma on i.ticketid = ma.recordkey and i.siteid = ma.siteid and ma.RECORDCLASS= 'ИНЦИДЕНТ' and ma.ISPRIMARY= 0 "
+"            where i.h='090004000002' and i.status!='УДАЛЕНО' "
+"  		 and ( 1=CASE WHEN   extract (year from i.reportdate) = extract (year from " + start + ")   "
+"								THEN 1 else 0 end )"
+" 			 and i.siteid   = " + siteid
+"            group by i.CLASSIDMAIN,  (case when i.extsystemid is not null then nvl( ma.pred, i.pred) else ma.pred end ),  i.status,  "
+"					( CASE WHEN  to_date (   i.reportdate,'DD.MM.YY') = " + start + " THEN 1 "
+"						else 0 end ) "
+"         ),  "
                  
+" inc_year_cl as (select  inc_year.pred,"

+"         sum(case when inc_year.CLASSIDMAIN = '090004000562' then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as razb_vsp, "
+"         sum(case when inc_year.CLASSIDMAIN = '090004000562' "
+"							and inc_year.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО','USTRANEN' ) then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as razb_vsp_ustr, "
+"         sum(case when inc_year.CLASSIDMAIN = '090004000562' and inc_year.today=1 "
+"							and inc_year.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО','USTRANEN' ) then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as razb_vsp_ustr_d, "
+"         sum(case when inc_year.CLASSIDMAIN in ('090001005513','090001003374','090004000558') then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as obr_girbic,     "
+"         sum(case when inc_year.CLASSIDMAIN in ('090001005513','090001003374','090004000558')  "
+"							and inc_year.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО','USTRANEN' ) then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as obr_girbic_ustr,     "
+"         sum(case when inc_year.CLASSIDMAIN in ('090001005513','090001003374','090004000558') and inc_year.today=1  "
+"							and inc_year.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО','USTRANEN' ) then nvl(inc_year.inc_count, 0) "
+"               else 0 end )as obr_girbic_ustr_d     "

+"             from inc_year  "
+"  group by inc_year.pred"
+"               ) "
      
+"       select loc.description, loc.location, loc.nom, loc.region,  "
+"				inc_year_cl.razb_vsp, inc_year_cl.obr_girbic,   "
+"				inc_year_cl.razb_vsp_ustr, inc_year_cl.razb_vsp_ustr_d, inc_year_cl.obr_girbic_ustr, inc_year_cl.obr_girbic_ustr_d "
+"               from loc "

+"         left join inc_year_cl on inc_year_cl.pred=loc.location  "

;

	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
     return (false);


row["description"] = maximoDataSet.getString("description");   
row["region"] = maximoDataSet.getString("region");   
row["nom"] = maximoDataSet.getInteger("nom");   
row["location"] = maximoDataSet.getString("location");   
row["razb_vsp"] = maximoDataSet.getInteger("razb_vsp");
row["razb_vsp_ustr"] = maximoDataSet.getInteger("razb_vsp_ustr"); 
row["obr_girbic"] = maximoDataSet.getInteger("obr_girbic"); 
row["obr_girbic_ustr"] = maximoDataSet.getInteger("obr_girbic_ustr"); 
row["razb_vsp_ustr_d"] = maximoDataSet.getInteger("razb_vsp_ustr_d");
row["obr_girbic_ustr_d"] = maximoDataSet.getInteger("obr_girbic_ustr_d");


return (true);]]></method>
