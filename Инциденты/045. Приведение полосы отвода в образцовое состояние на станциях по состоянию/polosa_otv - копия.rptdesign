            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	

var par = new String(params["par"]);



par = "to_date ('"+params["par"]+"','DD.MM.YY')"


	var sqlText = new String();

	sqlText =     " select trim (cast (extract (day from " + par + ") as char (10)))|| "
 +" case  "
 +"      when extract (month from " + par + ") = 1 then ' января ' "
 +"      when extract (month from " + par + ") = 2 then ' февраля ' "
 +"      when extract (month from " + par + ") = 3 then ' марта ' "
 +"      when extract (month from " + par + ") = 4 then ' апреля ' "
 +"      when extract (month from " + par + ") = 5 then ' мая ' "
 +"      when extract (month from " + par + ") = 6 then ' июня ' "
 +"      when extract (month from " + par + ") = 7 then ' июля ' "
 +"      when extract (month from " + par + ") = 8 then ' августа ' "
 +"      when extract (month from " + par + ") = 9 then ' сентября ' "
 +"      when extract (month from " + par + ") = 10 then ' октября ' "
 +"      when extract (month from " + par + ") = 11 then ' ноября ' "
 +"      when extract (month from " + par + ") = 12 then ' декабря ' "
 +"      else '' end|| "
 +" trim (cast (extract (year from " + par + ") as char (10)))|| "
 +" ' г.' DAT "
 +" from dual "
;

	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);

row["DAT"] = maximoDataSet.getString("DAT");


	return (true);]]></method>
