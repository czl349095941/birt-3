            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();




var start = new String(params["par"]);

start = "to_date ('"+params["par"]+"','DD.MM.YY')"

var finish = new String(finishStr);
finish = "to_date ('"+finishStr+"','DD.MM.YY HH24:MI:SS')"

var numvalue = new String(params["numvalue"]);
numvalue =(params["numvalue"]).replace("=","")

var type = new String(params["type"]);
type =(params["type"]).replace("=","")





sqlText =  " select "
+"      count( distinct i.ticketid) as inc_count"
+"		  ,l.description||case when spc.assetattrid = 'TIP_RAZMST' then ' (станция)' when spc.assetattrid = 'TIP_RAZM_MSP'  then ' (перегон)'  end  mesto "
+"        ,i.location as loc "
+"        ,spc2.location "
+"		, spc2.alnvalue||', '||spc3.alnvalue description "

+"      from maximo.tk_rep2 i "
+"          join maximo.LOCATIONSPEC spc  on (i.location = spc.location and i.siteid = spc.siteid "
+"          									and 1= case when "+type+"=1 and spc.assetattrid in ('TIP_RAZMST','TIP_RAZM_MSP') then 1"
+"															when "+type+"=2 and spc.assetattrid = 'TIP_RAZMST' then 1"
+"															when "+type+"=3 and spc.assetattrid = 'TIP_RAZM_MSP' then 1"
+"														else 0 end "
+"												and spc.numvalue=1) "
+"          join maximo.locations l on spc.location=l.location and spc.siteid=l.siteid "
 +"    			left join maximo.LOCATIONSPEC spc2 on (case when i.extsystemid is not null then  i.pred end ) = spc2.location and spc2.siteid = i.siteid "
 +"   									and spc2.ASSETATTRID = 'SNAME' "
 +"   			left join maximo.LOCATIONSPEC spc3 on  (case when i.extsystemid is not null then i.pred end ) = spc3.location and spc3.siteid = i.siteid  "
 +"   									and spc3.ASSETATTRID = 'SNAME2' "
  +"   			left join maximo.LOCATIONSPEC spc1 on  (case when i.extsystemid is not null then i.pred end ) = spc1.location and spc1.siteid = i.siteid  "
 +"   									and spc1.ASSETATTRID = 'TIP_PREDPR'  "
+"      where " 
 +"   ( 1=CASE WHEN i.extsystemid is not null and i.extsystemid != 703   THEN 1 "
 +" 				WHEN (i.extsystemid is null and i.h in ('090004000002','090004000003','090004000004') )  "
 +"					THEN 1 else 0 end )"
 +"		and to_date (   to_char  (i.reportdate,'DD.MM.YY HH24:MI:SS') ,'DD.MM.YY HH24:MI:SS') between " + start + " and  " + finish + " "
 +"  		and  i.status  not in ('ОБЪЕДИНЕН','ЛОЖНЫЙ', 'УДАЛЕНО') "
+"       	and i.siteid = '"+params["mroSite"]+"'"
+"			and spc1.NUMVALUE in ("+numvalue+")"
+"       group by l.description||case when spc.assetattrid = 'TIP_RAZMST' then ' (станция)' when spc.assetattrid = 'TIP_RAZM_MSP'  then ' (перегон)'  end" 
+"        ,i.location  "
+"        ,spc2.location "
+"		, spc2.alnvalue||', '||spc3.alnvalue  "


;


maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
     return (false);
     

row["inc_count"] = maximoDataSet.getInteger("inc_count");
row["mesto"] = maximoDataSet.getString("mesto");
row["loc"] = maximoDataSet.getString("loc");
row["location"] = maximoDataSet.getString("location");
row["description"] = maximoDataSet.getString("description");
  
return (true);]]></method>
