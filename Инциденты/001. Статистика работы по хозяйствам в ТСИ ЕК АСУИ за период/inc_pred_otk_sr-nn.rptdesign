            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();

var siteid = new String(params["userName"]);

siteid = " ( select distinct defsite from maximo.maxuser where loginid = '" + params["userName"] + "' )";
//siteid = "24";

var locationId = new String(params["locationId"]);

var status = new String(params["status"]);

var extsystemid = new String(params["extsystemid"]);

var start = new String(params["par"]);

start = "to_date ('"+params["par"]+"','DD.MM.YY')"

var finish = new String(params["fin"]);
finish = "to_date ('"+params["fin"]+"','DD.MM.YY')"



	sqlText = " select "
 +" locations.description|| "
 +" case when sp.numvalue=1 then ' (станция)' when sp1.numvalue =1  then ' (перегон)' else '' end  mesto, "
 +" i.ticketid ticketid, "
 +" w_ord.wonum workid, "
  +" i.reportdate rep, "
  +" (CASE WHEN i.extsysname is not null   THEN i.extsysname "
  +"              else person.displayname end ) displayname, "
  +" (CASE WHEN i.extsysname is not null    THEN '' "
  +"        else "
+"                '( '|| "
+"                                 (select distinct cr.description from MAXIMO.laborcraftrate lcr "
+"                                             join MAXIMO.CRAFT cr on cr.craft = lcr.craft "
+"                                 where lcr.DEFAULTCRAFT = 1 and lcr.LABORCODE =  person.personid "
+"                                 ) "
+"                      || ' )' "
  +"  end ) reg_title , "
 +" i.description vid , "
  +" a.description asset,	"
 +" i.TARGETFINISH srok, "
 +" NUMERICDOMAIN.description prio, "
 +" synd.description status,"
 +" nvl( i.ACTFINISH, i.changedate )  dat, "
 +" (case when ("+status+"=4 or "+status+"=6  ) and to_date(i.targetfinish,'DD.MM.YY') <= "+finish+"   THEN round("+finish+" + 1 - to_date(i.TARGETFINISH)) " 
 +"      when "+status+"=5 THEN round(to_date(nvl(i.changedate , i.ACTFINISH) )-to_date(i.TARGETFINISH)) " 
 +"        else 0 end) dat_pr,"

  +" (CASE WHEN  nvl(per_sh_p.personid, per_otk.personid) is null   THEN '' "
  +"        else "
+"                '( ' || "
+"                                 (select distinct cr.description from MAXIMO.laborcraftrate lcr "
+"                                             join MAXIMO.CRAFT cr on cr.craft = lcr.craft "
+"                                 where lcr.DEFAULTCRAFT = 1 and lcr.LABORCODE = nvl(per_sh_p.personid, per_otk.personid) "
+"                                 ) "
+"                      || ' )' "
  +"  end ) otv_title,  "

 +"	nvl(per_sh_p.displayname, per_otk.displayname) person_w,	"
 
  +" w_log.description reason	"
 +" from "
+" (	select i.reportdate, i.description,i.extsysname,i.reportedby,i.location , i.siteid, i.status ,i.ticketid,i.inc_priority, i.TARGETFINISH  , i.assetnum,  "
+"		min(s.changedate) as changedate,  MAX(ACTFINISH) as ACTFINISH "
  
+" 	  from "
+" 		( "
+" 			select i.reportdate, i.description,i.extsysname, i.reportedby, i.location , i.siteid, i.status ,i.ticketid,i.inc_priority, i.TARGETFINISH, ma.assetnum  "
+" 			from  "
+" 			  MAXIMO.incident i "

+" 			left  join (select RECORDKEY, RECORDCLASS, PODR, ISPRIMARY, SITEID, pred,assetnum from  MAXIMO.multiassetlocci "
+" 	                    ) ma on i.ticketid = ma.recordkey and i.siteid = ma.siteid and ma.RECORDCLASS= 'ИНЦИДЕНТ' and ma.ISPRIMARY= 0 "

    
 
 +" where  i.siteid=" + siteid + " and  (case when i.extsystemid is not null then nvl( ma.pred, i.pred) else ma.pred end ) = '"+locationId+"' "
 +"    and (  1=CASE "
 +" 		WHEN ("+status+"=4  and  i.status  in ('НОВОЕ','НАПР_В_ГР','ОТКЛОНЕН','ОТКЛЛОЖ','ОЖДИСП','В_РАБОТЕ','ПРОДОЛЖ')  and to_date (    i.targetfinish,'DD.MM.YY') <= "+finish+" "
 +"					) THEN 1 "
 +" 		WHEN ("+status+"=5  and  i.status in ( 'ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО','USTRANEN')"

 +"				) THEN 1 "
 
 +" 		WHEN ("+status+"=6 and  i.status != 'УДАЛЕНО'  and i.status = 'ОТКЛОНЕН' "
+"                      and to_date (    i.statusdate,'DD.MM.YY')  between   " + start + " and  " + finish + "  "  
+"				) THEN 1 "
+"				else 0 end)"
+"  and ( 1=CASE WHEN ("+extsystemid+"=7 and i.extsystemid is not null and i.extsystemid != 703) THEN 1 "
+" 			WHEN ("+extsystemid+"=0 and i.extsystemid is null and i.h in ('090000097755','090004000002','090004000003','090004000004') ) THEN 1 else 0 end )"

         
+" 	)i  "

+" 		    left  join maximo.tkstatus s  on "+status+"=5 and s.class='ИНЦИДЕНТ' and s.status in ('УСТРАНЕН','ЗАВ_ДИСП')   and s.ticketid=i.ticketid   "
+" 		    left   join maximo.workorder w  on  "+status+"=5 and w.origrecordid= i.ticketid  and w.origrecordclass='ИНЦИДЕНТ'  "
   
+" 		group by i.reportdate, i.description,i.extsysname, i.reportedby, i.location, i.siteid, i.status, i.ticketid, i.inc_priority, i.TARGETFINISH, i.assetnum  "
  
+" )i   "

 +"		      left       join MAXIMO.asset a on i.assetnum=a.assetnum and a.siteid=i.siteid	 "
 +"   left join maximo.person on  i.reportedby=person.personid "
 +"   left join maximo.locations on (locations.location=i.location and locations.siteid=i.siteid)  "
 +"   left join maximo.locationspec sp  "
 +"   on (i.location = sp.location and sp.siteid = i.siteid and sp.assetattrid = 'TIP_RAZMST') "
 +"   left join maximo.locationspec sp1  "
 +"   on (i.location = sp1.location and i.siteid = sp1.siteid and sp1.assetattrid = 'TIP_RAZM_MSP') "
 +"   left join MAXIMO.synonymdomain synd on i.status=synd.value and 'INCIDENTSTATUS'=synd.domainid"
 +"   left join maximo.NUMERICDOMAIN on (NUMERICDOMAIN.domainid='NSI_INC_PRIORITY' and    "
 +"   NUMERICDOMAIN.value=i.inc_priority )   "
 

 
 
  +"    left join   maximo.workorder  w_ord on  w_ord.origrecordid= i.ticketid  and w_ord.origrecordclass='ИНЦИДЕНТ'  "
  +"  	left join   maximo.person  per_sh_p on  per_sh_p.personid= w_ord.supervisor     and    per_sh_p.locationsite= i.siteid "
 +"		left join ( select description, recordkey from MAXIMO.worklog where logtype='ОТКЛОНЕНИЕ')w_log on w_log.recordkey=w_ord.wonum    "     

 +"			left join ( select p_work.displayname, p_work.personid, nvl(lpodr.L_PODR,  p_work.l_podr) l_podr, p_work.location pred  from maximo.maxuser m "
 +"							join maximo.person p_work on m.personid=p_work.personid "
 +"							left join maximo.lpodr on p_work.personid = lpodr.personid  "
 +"                 		join MAXIMO.GROUPUSER G on (G.USERID = m.USERID)	"
 +"               		where  G.GROUPNAME in ('CRAFT_RUKGRUPNAZ_E','CRAFT_RUKGRUPNAZ','CRAFT_SHNS','CRAFT_RUKGRUPNAZ_NGS') and "
 +"                      p_work.locationsite= " + siteid + " )per_otk on  per_otk.l_podr= w_ord.L_PODR  "        
   
 +"         where   1=case when "+status+"=5 and  "
 +"   to_date (    nvl(i.ACTFINISH, i.changedate),'DD.MM.YY')  between  " + start + " and  " + finish + "  "     
 +"  	and to_date ( nvl(i.ACTFINISH, i.changedate) ,'DD.MM.YY') > to_date (i.TARGETFINISH,'DD.MM.YY')  then 1 "
 +"   when "+status+" != 5 then 1 "
 +"   else 0 end  "
 
  ;


	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
     return (false);

row["MESTO"] = maximoDataSet.getString("MESTO");
row["TICKETID"] = maximoDataSet.getString("TICKETID");
row["REP"] = maximoDataSet.getString("REP");
row["DISPLAYNAME"] = maximoDataSet.getString("DISPLAYNAME");
row["VID"] = maximoDataSet.getString("VID");
row["SROK"] = maximoDataSet.getString("SROK");
row["PRIO"] = maximoDataSet.getString("PRIO");
row["STATUS"] = maximoDataSet.getString("STATUS");
row["DAT"] = maximoDataSet.getString("DAT");
row["DAT_PR"] = maximoDataSet.getString("DAT_PR");
row["person_w"] = maximoDataSet.getString("person_w");
row["workid"] = maximoDataSet.getString("workid");
row["reason"] = maximoDataSet.getString("reason");
row["reg_title"] = maximoDataSet.getString("reg_title");
row["otv_title"] = maximoDataSet.getString("otv_title");
row["asset"] = maximoDataSet.getString("asset");

return (true);]]></method>
