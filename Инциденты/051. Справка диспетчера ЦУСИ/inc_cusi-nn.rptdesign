            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	var sqlText = new String();
	
var siteid = new String(params["mroSite"]);
siteid = "'" + params["mroSite"] + "'" 


var locationId = new String("'" + params["locationId"] + "'");

var status = new String(params["status"]);

var sum_uch = new String(params["sum_uch"]);

var podrId = new String(params["podrId"]);
var podrId_ex = new String(1);

if(params["podrId"]!='')
{podrId = "'"+params["podrId"]+"'"}
else if(params["podrId"]=='')
{	podrId_ex = 0;
	podrId = 0;}
	
	

var start = new String(params["par"]);

start = "to_date ('"+params["par"]+"','DD.MM.YY')"

var finish = new String(finishStr);
finish = "to_date ('"+finishStr+"','DD.MM.YY HH24:MI:SS')"


	sqlText = " select distinct"
 +" locations.description|| "
 +" case when sp.numvalue=1 then ' (станция)' when sp1.numvalue =1  then ' (перегон)' else '' end  mesto, "
 +" i.ticketid ticketid, "
 +" w_ord.wonum workid, "
 +" i.reportdate  rep, "
 
  +" (CASE WHEN i.extsystemid is not null   THEN  "
  +"   				( select description from MAXIMO.numericdomain  where value=i.extsystemid and domainid = 'NSI_EXTSYSTEMS' ) "
  +"		else person.displayname end ) displayname, "
 
   +" (CASE WHEN i.extsystemid is not null   THEN '' "
  +"		else "
+"  			'( '|| "
+" 						(select distinct cr.description from MAXIMO.laborcraftrate lcr "
+"								join MAXIMO.CRAFT cr on cr.craft = lcr.craft "
+"						 where lcr.DEFAULTCRAFT = 1 and lcr.LABORCODE =  person.personid "
+"						) "
+"				|| ' )' "
  +"  end ) reg_title , "
 

 +" i.description vid , "
 +" a.description asset,	"
 +" i.TARGETFINISH srok, "
 +" NUMERICDOMAIN.description prio, "
 +" synd.description status,"
 +"	'дата статуса: '||i.statusdate statusdate, "
+"  w_ustr_st.ACTFINISH   dat, "
 +" ( case  when l_tip.NUMVALUE in ('3') then l_des_p.description "
 +"						 	 else l_des_e_sh.description end ) podr	"

+"		 from MAXIMO.TK_REP i		 "



 +"   left join maximo.person on  i.reportedby=person.personid "
 +"   left join maximo.locations on locations.location=i.location and locations.siteid=i.siteid  "
 +"   left join maximo.locationspec sp on locations.location = sp.location and locations.siteid = sp.siteid and sp.assetattrid = 'TIP_RAZMST' "
 +"   left join maximo.locationspec sp1  on locations.location = sp1.location and locations.siteid = sp1.siteid and sp1.assetattrid = 'TIP_RAZM_MSP' "

 +"   left join MAXIMO.synonymdomain synd on i.status=synd.value and 'INCIDENTSTATUS'=synd.domainid"
 +"   left join maximo.NUMERICDOMAIN on NUMERICDOMAIN.domainid='NSI_INC_PRIORITY' and  NUMERICDOMAIN.value=i.inc_priority    "
 
 +"   left  join MAXIMO.asset a on i.assetnum=a.assetnum and a.siteid=i.siteid	"

 
 
 +"   left join  maximo.workorder w_ord on  w_ord.origrecordid= i.ticketid  and w_ord.origrecordclass='ИНЦИДЕНТ' "
 
 +"   join MAXIMO.locationspec l_tip on   l_tip.location =  NVL (i.APRED, i.PRED) and l_tip.assetattrid='TIP_PREDPR' and l_tip.siteid  = " + siteid + "  "
                              
 +"   left  join (  "
 +"           select distinct connect_by_root(parent) ANCESTOR, siteid, location, systemid from MAXIMO.lochierarchy LH CONNECT BY     "
 +"             PRIOR  LH.LOCATION = LH.PARENT AND PRIOR LH.SYSTEMID = LH.SYSTEMID AND PRIOR LH.SITEID = LH.SITEID AND  level in (1,2,3) START WITH PARENT = "+locationId+"  and LH.siteid = " + siteid + " "

 +"            )l on  l.location= i.apodr  "
 +"             and l.SYSTEMID= case when l_tip.NUMVALUE ='3' then 'ОС' "
 +"                                  when l_tip.NUMVALUE ='4' then 'СЭ' "
 +"                                  when l_tip.NUMVALUE ='5' then 'Ш' else '' end "
 +"                          and l.ancestor = "+locationId+" "

 +"	left join  MAXIMO.locations l_des_e_sh on  l_des_e_sh.siteid = " + siteid + " and l_des_e_sh.location= i.apodr  "
 
 +"	left join ( select l.SITEID, l.LOCATION, l.SYSTEMID, l.parent, d.alnvalue as description  from MAXIMO.LOCHIERARCHY l"
 +"						join MAXIMO.locationspec d on d.location=l.parent and d.siteid=l.siteid and d.assetattrid= 'SNAME_PODR'"
 +"				)l_des_p on l_des_p.siteid = " + siteid + " and l_des_p.location= i.apodr  and l_des_p.SYSTEMID= 'ОС' "

                         
 +" left join 	(select ticketid , tksCHANGEDATE as ACTFINISH from maximo.tk_rep"
 +"                 where siteid = "+siteid+" and status in ('УСТРАНЕН', 'ЗАВ_ДИСП')  and h in ('090004000002','090004000003','090004000004') "
 +"                       and  tksCHANGEDATE >= " + start + " ) w_ustr_st  on w_ustr_st.ticketid= i.ticketid  "


+"	where  i.status != 'УДАЛЕНО'   and i.siteid = " + siteid + " "
+"	 			and i.extsystemid != 703   and i.h in ('090004000002','090004000003','090004000004')  "
+"  and 1 = case when "+status+"=0 and i.reportdate  between   " + start + " and  " + finish + "  then 1 "
+"  			 when "+status+"=7  and i.TARGETFINISH  between   " + start + " and  " + finish + "  then 1 "
+"  			 when "+status+" in (2,3,4,5,6,8,9,10,11,12) and  i.TKSCHANGEDATE BETWEEN   " + start + " AND " + finish + "    then 1  "
+"		else 0 end "

 

 +"		and   1=(case when "+podrId_ex+"=1 and "
 +"						 1= case when l_tip.NUMVALUE in ('4','5') and ( l_des_e_sh.description = "+podrId+" ) then 1"
 +"						 	    when l_tip.NUMVALUE in ('3') and ( l_des_p.description =  "+podrId+" ) then 1 "
 +"						 	 else 0 end "
 +"					then 1"

 +"						when "+podrId_ex+"=0 then 1"
 +"					else 0 end)" 
 
 
 +"		and   1=(case when "+sum_uch+"=1 and "
 +"								 1= case when l_tip.NUMVALUE in('4','5') and ( l_des_e_sh.description is not null ) then 1"
 +"						 	    		when l_tip.NUMVALUE in('3') and ( l_des_p.description is not null ) then 1 "
 +"						 	 		else 0 end "
 +"						 then 1"
 +"							when "+sum_uch+"=0 then 1"
 +"					else 0 end)" 
 
  
+"    and   1=CASE "
+"      	WHEN "+status+"=0    THEN 1 "

 +"  		WHEN "+status+"=2 "
+"	  				and  i.TKSSTATUS='НАПР_В_ГР'  "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
+"									and exists ( select 1 from MAXIMO.GROUPUSER  where USERID = i.tkschangeby and GROUPNAME ='CRAFT_STDISPCUSI' )  "

+"				THEN 1 "
 
 +"  		WHEN "+status+"=3  "
 +"					and i.TKSSTATUS='НАПР_В_ГР'  "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
+"	            THEN 1 "
 
  +"     	WHEN "+status+"=4  "
 +"        			and  i.TKSSTATUS='В_РАБОТЕ'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
+"	            THEN 1 "
 
 +"  		WHEN "+status+"=5 "
+"	  						and i.TKSSTATUS='В_РАБОТЕ'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "   "
                      
+"									and not exists ( select 1 from maximo.tkstatus s where i.ticketid=s.ticketid and s.class='ИНЦИДЕНТ' and s.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО')   "
+"														and s.changedate BETWEEN  " + start + " AND " + finish + "   "
+"													) "
+"				THEN 1 "
 
 +"  		WHEN "+status+"=6  "
+"	  						and  i.TKSSTATUS='В_РАБОТЕ'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
                      
+"									and not exists ( select 1 from maximo.tkstatus s where i.ticketid=s.ticketid and s.class='ИНЦИДЕНТ' and s.status in ('ЗАВ_ДИСП','УСТРАНЕН','ЗАКРЫТО')   "
+"													and s.changedate BETWEEN  " + start + " AND " + finish + "   "
+"												) "
                      
+"	                				and i.TARGETFINISH between " + start + " AND " + finish + "     "  
+"				THEN 1 "
 
  +"     	WHEN "+status+"=7  "
 +"        				and  NOT EXISTS  "
+"	             					(SELECT 1   FROM MAXIMO.TKSTATUS S  WHERE i.ticketid=s.ticketid and s.class='ИНЦИДЕНТ' "
+"	             						 and s.status  in ('УСТРАНЕН', 'ЗАВ_ДИСП', 'ЗАКРЫТО','ОБЪЕДИНЕН','ЛОЖНЫЙ') and S.CHANGEDATE between " + start + " AND " + finish + " "
+"	              					)  "                     
+"				THEN 1 "

 +"  		WHEN "+status+"=8 "
+"	  						and i.TKSSTATUS='ОТКЛОНЕН'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
+"	            THEN 1 "
 
 +"  		WHEN "+status+"=10 "
 +"	  						and  w_ustr_st.ACTFINISH   between " + start + " and  " + finish + " "
 +"				THEN 1  "
 
  +"     	WHEN "+status+"=11 "
 +"	  						and  w_ustr_st.ACTFINISH   between " + start + " and  " + finish + " "
+"									and  w_ustr_st.ACTFINISH >  i.TARGETFINISH "  
+"				THEN 1 "
 
 +"  		WHEN "+status+"=9 "
+"	  						and   i.TKSSTATUS='ЗАВ_ДИСП'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
                      
+"									and not exists ( select 1 from maximo.tkstatus s where i.ticketid=s.ticketid and s.class='ИНЦИДЕНТ' and s.status in ('УСТРАНЕН','ЗАКРЫТО')   "
+"														and s.changedate BETWEEN  " + start + " AND " + finish + "   "
+"	                       							 )  "
+"				THEN 1 "
 
 +"  		WHEN "+status+"=12  "
 +"						and  i.TKSSTATUS='ЗАКРЫТО'   "
+"									and i.tkschangedate BETWEEN  " + start + " AND " + finish + "  "
+"	            THEN 1 "

+"		 else 0 end"

 +"  and  NVL (i.APRED, i.PRED) = "+locationId+" "

 ;

	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
