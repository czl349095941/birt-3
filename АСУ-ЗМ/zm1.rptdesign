            <property name="queryText">with 

A1 as 
(

select  ID_VIOLATION, to_char (DATE_ZM, 'DD.MM.YY') dat  from TSI_MONITORING
where STATUS = -1 and DATE_ZM between ? and ?


),
a2 as
(
select T.ID_VIOLATION 
from TSI_MONITORING T
join 
(select max (DATE_ZM) DATE_ZM, ID_VIOLATION  from TSI_MONITORING where STATUS != -1  and ID_VIOLATION in (select ID_VIOLATION from A1)
group by ID_VIOLATION) 
AA on AA.DATE_ZM = T.DATE_ZM and T.ID_VIOLATION = AA.ID_VIOLATION
and T.is_error = 1
)
select A1.DAT, COUNT (A1.ID_VIOLATION) ALL_ID, COUNT (A2.ID_VIOLATION) ERR 
from A1 left join A2 on A1.ID_VIOLATION = A2.ID_VIOLATION
group by a1.dat
</property>
