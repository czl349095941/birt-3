	sqlText =   " select COUNT (TICKETID) ALL_ID, COUNT (distinct LOGTABLE.IDINEXTSYSTEM) err, "
	 +" TO_CHAR (EXTSYSTEM_REPORTDATE, 'DD.MM.YY') dat "
	 +" from MAXIMO.INCIDENT   "
	 +" left join maximo.logtable  on incident.idinextsystem = logtable.idinextsystem and object_type = 1011 and logtable.extsystem = 11 and message_type = 'ERROR' and incident.siteid = logtable.dor_kod "
