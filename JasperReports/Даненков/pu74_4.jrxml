	<queryString>
		<![CDATA[WITH WORKO AS
        (SELECT WO.WONUM ,
                wo.origrecordid,
                ACTFINISH,
                Wo.Description                       description,
                TO_CHAR (wo.Actfinish, 'dd.mm.yyyy') reportdate,
                WO.FAKTMEASURE ,
                WO.PLAN_WO ,
                WO.FAKT_WO ,
                wo.SITEID,
                ' (от ' || mult.STARTMEASUREKM || ' км ' || mult.STARTMEASUREPIK || ' пк ' || mult.STARTMEASUREM || ' м до ' || mult.ENDMEASUREKM || ' км ' || mult.ENDMEASUREPIK || ' пк ' || MULT.ENDMEASUREM || ' м) ' mesto ,
                MULT.ASSETNUM,
                mult.ASSESSMENT,
                MULT.STARTMEASUREKM+MULT.STARTMEASUREPIK/100+MULT.STARTMEASUREM/10000 STARTM,
                mult.endMEASUREKM  +mult.endMEASUREPIK/100+mult.endMEASUREM/10000     endm
        FROM    MAXIMO.WORKORDER WO
                LEFT JOIN MAXIMO.multiassetlocci mult
                        ON      wo.wonum             = mult.recordkey
                                AND MULT.RECORDCLASS = 'РАБОЧЕЕЗАДАНИЕ'
                                AND WO.SITEID        = MULT.SITEID
                                AND 1                = CASE WHEN (WO.ORIGRECORDID IS NOT NULL AND MULT.ISPRIMARY =1
                                                ) THEN 0 ELSE 1 END
        WHERE   WO.TYPEWORKORDER = 'Путевые обходчики'
                AND WO.STATUS IN ('НАПР_НА_УТ', 'ЗАВЕРШ', 'ОТКЛ_РГН', 'ОФОРМЛЕН', 'ЗАКРЫТО')
                AND WO.SITEID                     = replace($P{mroSite},'=','')
                AND WO.BRIGAD_LOCATION            = replace($P{podr},'=','')
                AND TO_CHAR(WO.ACTFINISH, 'YYYY') = replace($P{year},'=','')
                AND TO_CHAR(wo.Actfinish, 'MM')   = replace($P{month},'=','')
        )
        ,
        A1 AS
        ( SELECT ALR.TARGETASSETNUM,
                nkm.km,
                NKM.L
        FROM    WORKO
                JOIN MAXIMO.ASSETLOCRELATION ALR
                        ON      WORKO.SITEID       = ALR.SITEID
                                AND WORKO.ASSETNUM = ALR.TARGETASSETNUM
                JOIN MAXIMO.ASSET AST
                        ON      ALR.SITEID             = AST.SITEID
                                AND ALR.SOURCEASSETNUM = AST.ASSETNUM
                JOIN MAXIMO.ICP0_H_NKM NKM
                        ON      AST.SITEID            = NKM.SITEID
                                AND AST.IDINEXTSYSTEM = NKM.PUTGL_ID
                                AND NKM.KM BETWEEN ALR.SOURCESTARTMEASURE AND ALR.SOURCEENDMEASURE
                                AND SYSDATE BETWEEN nkm.date_nd AND nkm.date_kd
        GROUP BY ALR.TARGETASSETNUM,
                NKM.KM,
                NKM.L
        )
        ,
        labtr AS
        ( SELECT LABTRANS.REFWO ,
                LABTRANS.SITEID ,
                SYS_XMLAGG(xmlelement(COL, PERSON.DISPLAYNAME || ' (' ||PERSON.TPPERSON || '), ')).extract('/ROWSET/COL/text()').GETCLOBVAL() FIO
        FROM    MAXIMO.LABTRANS
                JOIN MAXIMO.LABORCRAFTRATE
                        ON      LABORCRAFTRATE.LABORCODE = LABTRANS.LABORCODE
                JOIN MAXIMO.PERSON
                        ON      PERSON.PERSONID = LABTRANS.LABORCODE
        WHERE   LABTRANS.LABORCODE    IS NOT NULL
                AND LABORCRAFTRATE.DEFAULTCRAFT = '1'
                AND
                (
                        LABTRANS.REFWO, LABTRANS.SITEID
                )
                IN
                (SELECT WONUM, siteid
                FROM    WORKO
                )
        GROUP BY LABTRANS.REFWO ,
                LABTRANS.SITEID
        )
SELECT  AST.ASSETNUM,
        to_char(labtr.FIO) fio ,
        TO_CHAR (SUBSTR((REGEXP_REPLACE(AST.DESCRIPTION, '\"', '') || worko.mesto),1,2000)) MESTO ,
        WORKO.DESCRIPTION ,
        WORKO.REPORTDATE , (case when AST.ITEMNUM in ('ГЛПУТЬ', 'СТАНПУТЬ', 'ПУТЬП', 'ГППС') then 'км' else 'шт' end) FAKTMEASURE,
        case when AST.ITEMNUM in ('ГЛПУТЬ', 'СТАНПУТЬ', 'ПУТЬП', 'ГППС') then (NVL((NVL((select L/1000 from    A1 where   A1.TARGETASSETNUM = WORKO.ASSETNUM and A1.KM = FLOOR(WORKO.STARTM)) ,0.1)-MOD(WORKO.STARTM, 1))*10000 + ((FLOOR(WORKO.ENDM) - CEIL(WORKO.STARTM))*1000
                                        +(select SUM(case when A1.KM between CEIL(WORKO.STARTM) and FLOOR(WORKO.ENDM)-1 then A1.L-1000 else 0 end) from A1 where   A1.TARGETASSETNUM = WORKO.ASSETNUM))
                                        + MOD(worko.endm, 1)*10000, ((MOD(WORKO.ENDM, 1)*10000+FLOOR(WORKO.ENDM)*1000) - (MOD(WORKO.STARTM, 1)*10000+FLOOR(WORKO.STARTM)*1000)))/1000) ELSE 1 END Zad ,
        Worko.PLAN_WO                                                                                                                                                               Vup ,
        worko.ASSESSMENT
FROM    worko
        LEFT JOIN maximo.asset ast
                ON      worko.assetnum   =ast.assetnum
                        AND worko.SITEID = AST.SITEID
        JOIN LABTR
                on      WORKO.WONUM      = LABTR.REFWO
                        AND WORKO.SITEID = LABTR.SITEID
ORDER BY to_char(labtr.FIO)]]>
	</queryString>
