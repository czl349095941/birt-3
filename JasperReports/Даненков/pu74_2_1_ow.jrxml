	<queryString>
		<![CDATA[WITH a1 AS
        (SELECT wo.wonum ,
                wo.PASSEGEWORK prohod,
                wo.PASSTRAIN   poezd,
                WO.FENCINGPATH ograda,
                WO.TYPEWORKORDER,
                wo.otherwork,
                wo.CLASSSTRUCTUREID,
                TO_CHAR (SUBSTR (
                                   (SELECT sys_xmlagg(XMLELEMENT(col, regexp_REPLACE(asset.description, '"', '')
                                                   || ' (от '
                                                   || multiassetlocci.STARTMEASUREKM
                                                   || ' км '
                                                   || multiassetlocci.STARTMEASUREPIK
                                                   || ' пк '
                                                   || multiassetlocci.STARTMEASUREM
                                                   || ' м до '
                                                   || multiassetlocci.ENDMEASUREKM
                                                   || ' км '
                                                   || multiassetlocci.ENDMEASUREPIK
                                                   || ' пк '
                                                   || multiassetlocci.ENDMEASUREM
                                                   || ' м) ')).EXTRACT('/ROWSET/COL/text()').getclobval()
                                   FROM    MAXIMO.multiassetlocci
                                           JOIN maximo.asset
                                                   ON      asset.assetnum   = multiassetlocci.assetnum
                                                           AND asset.siteid = multiassetlocci.siteid
                                   WHERE   multiassetlocci.recordkey        = wo.wonum
                                           AND 1                            = CASE WHEN (wo.origrecordid IS NOT NULL AND multiassetlocci.ISPRIMARY =1
                                                           ) THEN 0 ELSE 1 END
                                           AND multiassetlocci.recordclass = 'РАБОЧЕЕЗАДАНИЕ'
                                   ),1,2000)) mesto,
                JOBPLAN.JPNUM                 tk ,
                J1.Numvalue
                        || ' '
                        || J1.Measureunitid Ед_Изм ,
                JOBPLAN.DESCRIPTION наименов,
                wo.description rz ,
                wo.Jpnum Тех_Карта ,
                CASE WHEN NVL(wo.JPDURATION, 0) = 0 THEN WO.NOKOEFDURATION ELSE wo.JPDURATION END JPDURATION ,
                J1.Numvalue ,
                extract (DAY FROM wo.Actfinish) Actfinish ,
                wo.fakt_wo ,
                wo.labor_hour ,
                Wo.Plan_Wo
        FROM    Maximo.WORKORDER wo
                LEFT JOIN MAXIMO.wojprelation jpr
                        ON      wo.wonum               = jpr.wonum
                                AND jpr.usethismeasure = '1'
                LEFT JOIN Maximo.Jobplan
                        ON      Jobplan.jobplanid = jpr.jobplanid
                LEFT JOIN Maximo.Jobplanspec J1
                        ON      J1.jpnum            = jobplan.jpnum
                                AND J1.assetattrid  = 'IZMRAB'
        WHERE   wo.BRIGAD_LOCATION                  = replace($P{podr},'=','')
                AND wo.siteid                       = replace($P{mroSite},'=','')
                AND extract (DAY FROM wo.Actfinish) < 16
                AND wo.Istask                       = '0'
                AND TYPEWORKORDER IN ('Внеплановое', 'Плановое', 'Сопутствующая работа', 'Прочее', 'Путевые обходчики')
                AND wo.STATUS     IN ('НАПР_НА_УТ', 'ЗАВЕРШ', 'ОТКЛ_РГН', 'ОФОРМЛЕН', 'ЗАКРЫТО')
                AND TO_CHAR(wo.Actfinish, 'MM')   = replace($P{month},'=','')
                AND TO_CHAR(wo.Actfinish, 'YYYY') = replace($P{year},'=','')
        )
SELECT  'invis' MESTO ,
        '' Ед_Изм ,
        NVL( CASE WHEN TYPEWORKORDER IN ('Путевые обходчики') THEN 'Осмотр пути' ELSE NVL(a1.OTHERWORK, cls.description) END, 'Прочие работы') Наименов,
        'я' Тех_Карта ,
        to_number('') Норма_Времени ,
        to_number('')                                                        numvalue ,
        SUM (a1.Plan_wo)                                                     PLAN ,
        ROUND(SUM((a1.Plan_wo / a1.numvalue) * To_number(a1.JPDURATION)), 2) AS Норм_ч_месц_плн,
        SUM ( CASE WHEN Actfinish=1 THEN fakt_wo END)За_1_Число,
        SUM ( CASE WHEN Actfinish=2 THEN fakt_wo END)За_2_Число,
        SUM ( CASE WHEN Actfinish=3 THEN fakt_wo END)За_3_Число,
        SUM ( CASE WHEN Actfinish=4 THEN fakt_wo END)За_4_Число,
        SUM ( CASE WHEN Actfinish=5 THEN fakt_wo END)За_5_Число,
        SUM ( CASE WHEN Actfinish=6 THEN fakt_wo END)За_6_Число,
        SUM ( CASE WHEN Actfinish=7 THEN fakt_wo END)За_7_Число,
        SUM ( CASE WHEN Actfinish=8 THEN fakt_wo END)За_8_Число,
        SUM ( CASE WHEN Actfinish=9 THEN fakt_wo END)За_9_Число,
        SUM ( CASE WHEN Actfinish=10 THEN fakt_wo END)За_10_Число,
        SUM ( CASE WHEN Actfinish=11 THEN fakt_wo END)За_11_Число,
        SUM ( CASE WHEN Actfinish=12 THEN fakt_wo END)За_12_Число,
        SUM ( CASE WHEN Actfinish=13 THEN fakt_wo END)За_13_Число,
        SUM ( CASE WHEN Actfinish=14 THEN fakt_wo END)За_14_Число,
        SUM ( CASE WHEN Actfinish=15 THEN fakt_wo END)За_15_Число,
        SUM ( CASE WHEN Actfinish=1 THEN labor_hour END)Ч_Ч_1,
        SUM ( CASE WHEN Actfinish=2 THEN labor_hour END)Ч_Ч_2,
        SUM ( CASE WHEN Actfinish=3 THEN labor_hour END)Ч_Ч_3,
        SUM ( CASE WHEN Actfinish=4 THEN labor_hour END)Ч_Ч_4,
        SUM ( CASE WHEN Actfinish=5 THEN labor_hour END)Ч_Ч_5,
        SUM ( CASE WHEN Actfinish=6 THEN labor_hour END)Ч_Ч_6,
        SUM ( CASE WHEN Actfinish=7 THEN labor_hour END)Ч_Ч_7,
        SUM ( CASE WHEN Actfinish=8 THEN labor_hour END)Ч_Ч_8,
        SUM ( CASE WHEN Actfinish=9 THEN labor_hour END)Ч_Ч_9,
        SUM ( CASE WHEN Actfinish=10 THEN labor_hour END)Ч_Ч_10,
        SUM ( CASE WHEN Actfinish=11 THEN labor_hour END)Ч_Ч_11,
        SUM ( CASE WHEN Actfinish=12 THEN labor_hour END)Ч_Ч_12,
        SUM ( CASE WHEN Actfinish=13 THEN labor_hour END)Ч_Ч_13,
        SUM ( CASE WHEN Actfinish=14 THEN labor_hour END)Ч_Ч_14,
        SUM ( CASE WHEN Actfinish=15 THEN labor_hour END)Ч_Ч_15
FROM    a1
        LEFT JOIN MAXIMO.CLASSSTRUCTURE cls
                ON      cls.CLASSSTRUCTUREID = a1.CLASSSTRUCTUREID
where   TYPEWORKORDER in ('Прочее', 'Путевые обходчики')
GROUP BY NVL( CASE WHEN TYPEWORKORDER IN ('Путевые обходчики') THEN 'Осмотр пути' ELSE NVL(a1.OTHERWORK, cls.description) END, 'Прочие работы')
