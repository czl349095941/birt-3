  AND wo.siteid                       = '17'
  AND extract (DAY FROM wo.Actfinish) < 16
  AND wo.Istask                       = '0'
  AND TYPEWORKORDER                  IN ('Внеплановое', 'Плановое', 'Сопутствующая работа', 'Прочее')
  AND wo.STATUS                      IN ( 'ТЕХОТДЕЛ' , 'ЗАКРЫТО', 'ЗАВЕРШ', 'ДОРАБОТКА' )
