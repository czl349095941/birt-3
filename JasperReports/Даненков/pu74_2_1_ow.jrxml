    JOIN maximo.asset
    ON asset.assetnum               = multiassetlocci.assetnum
    AND asset.siteid                = multiassetlocci.siteid
    WHERE multiassetlocci.recordkey = wo.wonum
    AND multiassetlocci.recordclass = 'РАБОЧЕЕЗАДАНИЕ'
