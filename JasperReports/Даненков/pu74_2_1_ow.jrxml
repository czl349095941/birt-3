    AND PODR.SITEID     = '17'
    AND LOC1.STATUS     = 'РАБОТАЕТ'
    AND LOC2.STATUS     = 'РАБОТАЕТ'
    AND PRED.SYSTEMID   = 'ОС'
    AND podr.systemid   = 'ОС'
