	<queryString>
		<![CDATA[with KONT as
(
select  COUNT (distinct LABTRANS.LABORCODE) FK,
        SUM(LABTRANS.labor_hour)            fk_hour
FROM    MAXIMO.LABTRANS
        JOIN MAXIMO.LABORCRAFTRATE
                ON      LABORCRAFTRATE.LABORCODE = LABTRANS.LABORCODE
WHERE   LABTRANS.LABORCODE             IS NOT NULL
        AND LABTRANS.labor_hour                  > 0
        AND LABORCRAFTRATE.DEFAULTCRAFT          = '1'
        AND LABTRANS.SITEID                      = '24'
        AND LABTRANS.REFWO IN
        (SELECT wonum
        FROM    Maximo.Workorder Wo
        WHERE   WO.SITEID                         = '24'
                AND TO_CHAR(wo.Actfinish, 'MM')   = 04
                AND TO_CHAR(wo.Actfinish, 'YYYY') = '2014'
                AND WO.BRIGAD_LOCATION            = 'PODR_108'
                AND WO.STATUS     IN ('НАПР_НА_УТ', 'ЗАВЕРШ', 'ОТКЛ_РГН', 'ОФОРМЛЕН', 'ЗАКРЫТО')
                AND TYPEWORKORDER IN ('Внеплановое', 'Плановое', 'Сопутствующая работа', 'Прочее', 'Путевые обходчики')
        )
)
SELECT  pd.Description   pd ,
        pchu.Description pchu ,
        PCH.DESCRIPTION  PCH ,
        SITE.DESCRIPTION SITEID ,
        (SELECT COUNT (*)
        FROM    maximo.person
                JOIN maximo.LABORCRAFTRATE
                        ON      person.personid                 = LABORCRAFTRATE.laborcode
                                AND LABORCRAFTRATE.DEFAULTCRAFT = 1
                                AND LABORCRAFTRATE.craft IN ('00000205', 'МНТР_ПТ', 'СГНЛСТ', '00000346')
        WHERE   person.l_podr           = pd.location
                AND person.locationsite = pd.SITEID
                AND person.status       = 'АКТИВНО'
        )                RASCH_KONTINGENT,
        (select FK from KONT) FK,
        (select fk_hour from KONT) FK_hour,
        (SELECT  ' на ' || (
                CASE
                        WHEN 07 = 01 THEN ' январь '
                        WHEN 07 = 02 THEN ' февраль '
                        WHEN 07 = 03 THEN ' март '
                        WHEN 07 = 04 THEN ' апрель '
                        WHEN 07 = 05 THEN ' май '
                        WHEN 07 = 06 THEN ' июнь '
                        WHEN 07 = 07 THEN ' июль '
                        WHEN 07 = 08 THEN ' август '
                        WHEN 07 = 09 THEN ' сентябрь '
                        WHEN 07 = 10 THEN ' октябрь '
                        WHEN 07 = 11 THEN ' ноябрь '
                        WHEN 07 = 12 THEN ' декабрь '
                        ELSE ''
                END || 2014) || ' г.' DAT
FROM    dual) mon,
        DORM.DISPLAYNAME DORM ,
        norm.Displayname norm
FROM    maximo.locations pd
        JOIN MAXIMO.lochierarchy lh_pd
                ON      pd.location   = lh_pd.location
                        AND pd.siteid = lh_pd.siteid
        JOIN maximo.locations pchu
                ON      lh_pd.parent     = pchu.location
                        AND lh_pd.siteid = pchu.siteid
        JOIN MAXIMO.lochierarchy lh_pchu
                ON      pchu.location   = lh_pchu.location
                        AND pchu.siteid = lh_pchu.siteid
        JOIN maximo.locations pch
                ON      lh_pchu.parent     = pch.location
                        AND lh_pchu.siteid = pch.siteid
        JOIN maximo.site
                ON      PD.SITEID = SITE.SITEID
        LEFT JOIN
                (SELECT lpodr.l_podr,
                        person.locationsite siteid,
                        TO_CHAR(SYS_XMLAGG(xmlelement(COL, person.Displayname
                                || ', ')).extract('/ROWSET/COL/text()').GETCLOBVAL()) Displayname
                FROM    MAXIMO.LPODR
                        JOIN maximo.person
                                ON      LPODR.PERSONID = PERSON.PERSONID
                        JOIN Maximo.Laborcraftrate
                                ON      PERSON.PERSONID = LABORCRAFTRATE.LABORCODE
                        JOIN MAXIMO.CRAFT
                                ON      LABORCRAFTRATE.CRAFT = CRAFT.CRAFT
                WHERE   LABORCRAFTRATE.CRAFT IN ('СТМАСТЕР_ДОР', '00000640')
                        AND LABORCRAFTRATE.DEFAULTCRAFT = '1'
                        AND PERSON.STATUS               = 'АКТИВНО'
                        AND lpodr.l_podr      IS NOT NULL
                GROUP BY lpodr.l_podr,
                        person.locationsite
                ) dorm
                ON      pd.location   = dorm.l_podr
                        AND pd.siteid = dorm.siteid
        LEFT JOIN
                (SELECT lpodr.l_podr,
                        person.locationsite siteid,
                        TO_CHAR(SYS_XMLAGG(xmlelement(COL, person.Displayname
                                || ', ')).extract('/ROWSET/COL/text()').GETCLOBVAL()) Displayname
                FROM    MAXIMO.LPODR
                        JOIN maximo.person
                                ON      LPODR.PERSONID = PERSON.PERSONID
                        JOIN Maximo.Laborcraftrate
                                ON      PERSON.PERSONID = LABORCRAFTRATE.LABORCODE
                        JOIN MAXIMO.CRAFT
                                ON      LABORCRAFTRATE.CRAFT = CRAFT.CRAFT
                WHERE   LABORCRAFTRATE.CRAFT IN ('ИНЖ_НРМТРД', '00000576', 'ВЕДИНЖ_НРМТРД', '00003979')
                        AND LABORCRAFTRATE.DEFAULTCRAFT = '1'
                        AND PERSON.STATUS               = 'АКТИВНО'
                        AND lpodr.l_podr      IS NOT NULL
                GROUP BY lpodr.l_podr,
                        person.locationsite
                ) NORM
                ON      pd.location   = norm.l_podr
                        AND pd.siteid = norm.siteid
WHERE   pd.location                   = 'PODR_108'
        AND pd.SITEID                 = '24']]>
	</queryString>
