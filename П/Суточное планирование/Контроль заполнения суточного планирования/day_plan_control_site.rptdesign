            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
maximoDataSet.open();

var sqlText = new String();

sqlText = "" 
sqlText = "" 
+"SELECT  DESCRIPTION site_d, " 
+"        CASE "+params["month"] 
+"                WHEN 1 THEN ' январь ' " 
+"                WHEN 2 THEN ' февраль ' " 
+"                WHEN 3 THEN ' март ' " 
+"                WHEN 4 THEN ' апрель ' " 
+"                WHEN 5 THEN ' май ' " 
+"                WHEN 6 THEN ' июнь ' " 
+"                WHEN 7 THEN ' июль ' " 
+"                WHEN 8 THEN ' август ' " 
+"                WHEN 9 THEN ' сентябрь ' " 
+"                WHEN 10 THEN ' октябрь ' " 
+"                WHEN 11 THEN ' ноябрь ' " 
+"                WHEN 12 THEN ' декабрь ' " 
+"                ELSE '' " 
+"        END mmonth " 
+"FROM    MAXIMO.SITE " 
+"WHERE   SITEID = '"+params["mroSite"]+"' "
;


maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	return (false);

row["SITE_D"] = maximoDataSet.getString("SITE_D");
row["MMONTH"] = maximoDataSet.getString("MMONTH");


	
return (true);]]></method>
