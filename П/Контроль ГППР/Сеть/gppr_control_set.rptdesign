            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();
	
	


if(params["year"]=='' || params["year"] == null || params["year"] == '""' )
{params["year"]= BirtDateTime.year(BirtDateTime.now());}

params["stolb"] = (params["stolb"]).replace(/=/g,"");

var sqlText = new String();
if (params["stolb"]==1)
{wher = "p.status = '0' and pm.c is null";}

else if (params["stolb"]==2)
{wher = "p.status = '0' and pm.c is not  null";}

else if (params["stolb"]==3)
{wher = "p.status in ('1', '2', '3', '4', '5', '6', '15', '28', '14', '27')";}

else if (params["stolb"]==4)
{wher = "p.status = '7'";}

else if (params["stolb"]==5)
{wher = "p.status = '8'";}

sqlText = "Select	distinct P.Namerab1 "
+" 		, min(P.Description) Description "
+"		, to_number(abs(regexp_replace (substr (p.shifr, 0,2 ), '[^0-9]+', ''))) ord "
+"		, p.shifr "
+" 		, site.Siteid "
+" 		, Site.Description 		Sd "
+"      , site.d 											loc "
+" 		,sum (case when "+wher+" then 1 else 0 end )	stolb  "
+"  "
+" from	( "
+" 		select	masterpm.masterpmnum "
+" 				, masterpm.description "
+" 				, Namerab1 "
+" 				, masterpm.COORDSTATUS	status "
+" 				, Siteid "
+"                 , masterpm.location  "
+"				, masterpm.SHIFR "
+" 								 "
+" 		From	Maximo.Masterpm "
+" 				left join maximo.locationspec loc on loc.location = masterpm.location and loc.numvalue = 3 And loc.Assetattrid = 'TIP_PREDPR' "
+" 		Where 	"//Namerab1 In ('090000070802','090000070833','090000070834','090000070835','090000070864','090000070866','090000070867','090000070868','090000070869','090000070870','090000070871','090000070865','090000070872','090000070881','090000074801','090000070819','090000070826','090000074825','090000070799','090000070804','090000070806','090000070808','090000070811','090000070815','090000070813','090000070817','090000070823','090000070824','090000070831','090000070832','090000070837','090000070848','090000070850','090000070851','090000070852','090000070903','090000070904','090000070905','090000070906','090000070907','090000070908','090000070909','090000070853','090000070854','090000070855','090000070856','090000070857','090000070858','090000070859','090000070860','090000070861','090000070862','090000070874','090000070376','090000070883','090000070884','090000070885','090000070886','090000070887','090000070888','090000070889','090000070890','090000070891','090000070892','090000070893','090000070894','090000070895','090000070896','090000070897','090000070898','090000070899','090000070900','090000070901','090000070902','090000074833', '090000074834') and"
+" 				 Loc.Siteid is not null "
+" 				and MASTERPM.year = '" + params["year"]+"'"
+" 		)P  "
+" 		Left Join	(Select pm.masterpm, '1' c  "
+"                     From Maximo.Pm  "
+"                     where pm.masterpm is not null  "
+"                     group by pm.masterpm "
+"                     )pm on  p.masterpmnum = pm.masterpm  "
+" 		 join (Select site.Siteid , site.description, count(loc.location) D  "
+"       From Maximo.Site  "
+"       left join maximo.locationspec loc on loc.siteid = site.siteid and loc.numvalue = 3 And loc.Assetattrid = 'TIP_PREDPR'  "
+"       Where Site.Orgid = 'РЖД'  "
+"			   and site.siteid <> '99'"
+"       group by site.siteid, site.description)Site on p.siteid = site.siteid  "
+""

+" 		Group By	P.Namerab1 "
+" 					, site.siteid "
+" 					,Site.Description"
+"          		,site.d"
+"					,to_number(abs(regexp_replace (substr (p.shifr, 0,2 ), '[^0-9]+', ''))) "
+"					, p.shifr"
+""
+"		ORDER BY ord";






maximoDataSet.setQuery(sqlText);
;


	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);
	   
row["NAMERAB1"] = maximoDataSet.getString("NAMERAB1");
row["DESCRIPTION"] = maximoDataSet.getString("DESCRIPTION");
row["ORD"] = maximoDataSet.getString("ORD");
row["SITEID"] = maximoDataSet.getString("SITEID");
row["SD"] = maximoDataSet.getString("SD");
row["LOC"] = maximoDataSet.getString("LOC");
row["STOLB"] = maximoDataSet.getString("STOLB");
row["SHIFR"] = maximoDataSet.getString("SHIFR");


	   
	return (true);]]></method>
