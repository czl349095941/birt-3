row["NAMERAB1"] = maximoDataSet.getString("NAMERAB1");
row["DESCRIPTION"] = maximoDataSet.getString("DESCRIPTION");
row["LOC"] = maximoDataSet.getString("LOC");
row["N"] = maximoDataSet.getString("N");



	   
	return (true);]]></method>
