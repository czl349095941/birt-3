            <method name="open"><![CDATA[maximoDataSet = MXReportDataSetProvider.create(this.getDataSource().getName(), this.getName());
	maximoDataSet.open();

var where = new String(params["where"]);
params["where"]="";

if(params["year"]=='')
{params["where"]+= " extract (year from sysdate)";}
else if(params["year"]!='')
{params["where"]+= params["year"]}

var status = new String(params["status"]);

if(params["status"]!='')
{status = " and " + MXReportSqlFormat.createParamWhereClause("masterpm.coordstatus",params["status"].toUpperCase())}
else if(params["status"]=='')
{status = " and 1 = 1 "};

	var sqlText = new String();

	sqlText = "  select  "
 +"  nvl (Reg.an_descr,'Не определено') an_descr , locspec.numvalue as ph , "
 +"  asset.description||' '||PM.STARTMEASUREKM||'м. '|| PM.STARTMEASUREPIK ||'пк. - '||	PM.ENDMEASUREKM ||'м. '|| PM.ENDMEASUREPIK ||'пк. ' as peregon, "
 +"  n1.description||n2.description||n3.description tip,  "
 +"  spec4.numvalue rad, "
 +"  longdescription.ldtext prim  "
 +" from maximo.masterpm   "
 +" left outer join maximo.locations on masterpm.location=locations.location  "
 +" left join maximo.locationspec locspec on (locspec.location = locations.location) and  locspec.ASSETATTRID = 'NOM' "
 +" left outer join maximo.pm on masterpm.masterpmnum = pm.masterpm   "
 +" left outer join maximo.asset on pm.assetnum = asset.assetnum  "
 +" left join maximo.assetspec spec1 on (maximo.asset.assetnum = spec1.assetnum AND spec1.ASSETATTRID = 'ID_REL')  "
 +" left join maximo.assetspec spec2 on (maximo.asset.assetnum = spec2.assetnum AND spec2.ASSETATTRID = 'ID_MK')  "
 +" left join maximo.assetspec spec3 on (maximo.asset.assetnum = spec3.assetnum AND spec3.ASSETATTRID = 'STP_ID_TYPE')  "
 +" left join( "
 +"             select A2.TARGETASSETNUM ts2, a1.targetassetnum ts1, a.siteid  "
 +"             from MAXIMO.ASSETLOCRELATION A1 "
 +"             join MAXIMO.ASSETLOCRELATIONSPEC AS1 on A1.SOURCEASSETNUM = AS1.SOURCEASSETNUM  "
 +"             and A1.TARGETASSETNUM = AS1.TARGETASSETNUM and A1.ASSETRELATIONNUM = AS1.ASSETRELATIONNUM and AS1.ASSETATTRID = 'BOOL_LEGPUT' and AS1.NUMVALUE =1 "
 +"             and a1.siteid = as1.siteid "
 +"             join MAXIMO.ASSET a on A1.SOURCEASSETNUM = a.ASSETNUM and A1.SITEID = a.SITEID  "
 +"             join MAXIMO.ASSETLOCRELATION A2 on A1.SOURCEASSETNUM = A2.SOURCEASSETNUM and A1.SITEID = A2.SITEID  "
 +"             and A2.ASSETRELATIONNUM = 'ПУТЬ_КРИВАЯ' and A1.SOURCESTARTMEASURE between A2.SOURCESTARTMEASURE and A2.SOURCEENDMEASURE "
 +"             where  a.itemnum = 'ГЛПУТЬ'  ) KRIV on KRIV.ts1=asset.assetnum and KRIV.siteid=asset.siteid "
 +"   left join maximo.assetspec spec4 on (KRIV.ts2 = spec4.assetnum AND spec4.ASSETATTRID = 'KRIV_R')  "
 +"   left join maximo.numericdomain n1 on (spec1.numvalue = n1.value and n1.domainid = 'NSI_HD_10_0')  "
 +"   left join maximo.numericdomain n2 on (spec2.numvalue = n2.value and n2.domainid = 'NSI_HD_32_0')  "
 +"   left join maximo.numericdomain n3 on (spec3.numvalue = n3.value and n3.domainid = 'NSI_HD_22_0')  "
 +"   left join MAXIMO.longdescription on (pm.pmuid = longdescription.ldkey and longdescription.ldownertable = 'PM')   "
 +"                       left join (           "
 +"                              select l1.location, l1.parent,  l1.siteid,           "
 +"                  					'Регион '||ln.NUMVALUE||', '|| ls.alnvalue as  an_descr          "
 +"                              from MAXIMO.lochierarchy l1           "
 +"                                join (select * from MAXIMO.locationspec where assetattrid = 'TIP_PREDPR' and numvalue = 2) l2 on l2.location = l1.parent and l2.siteid = l1.siteid           "
 +"                                join  MAXIMO.locationspec ls on ls.assetattrid = 'SNAME2' and ls.location = l1.parent and ls.siteid = l1.siteid          "
 +"                                join  MAXIMO.locationspec ln on ln.assetattrid = 'NOM' and ln.location = l1.parent and ln.siteid = l1.siteid          "
 +"                            ) Reg on (masterpm.location = Reg.location and Reg.siteid ='"+ params["mroSite"]+"')  "
 +"   where masterpm.namerab1 = '090000070848'  "
 +"   and pm.siteid ='"+ params["mroSite"]+"'"+ status
 +"   and masterpm.year =" + params["where"] 
 +"   and pm.month is not null  "

;
	maximoDataSet.setQuery(sqlText);]]></method>
            <method name="fetch"><![CDATA[if (!maximoDataSet.fetch())
	   return (false);
	// Add a line for each output column
row["PH"] = maximoDataSet.getString("PH");
row["NOD_DESC"] = maximoDataSet.getString("an_descr");
row["PEREGON"] = maximoDataSet.getString("PEREGON");
row["TIP"] = maximoDataSet.getString("TIP");
row["RAD"] = maximoDataSet.getString("RAD");
row["PRIM"] = maximoDataSet.getString("PRIM");

	return (true);]]></method>
